@using KisiselWebProjesi.Models.Classes
@model List<AnaSayfa>
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Ahmet Oğuzhan Engin</title>
    <link href="~/web-KisiselWebSitesi/css/style.css" rel="stylesheet" />


    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css">
</head>
<body>
    <div class="center">
        <div class="avatar">
            @foreach (var x in Model)
            {
                <img src="@x.profil">
            }
         
        </div>
        <div class="content">
            @foreach (var item in Model)
            {
                <h1>@item.İsim</h1>
                <h2>@item.unvan</h2>
                <p>@item.aciklama</p>
                <h3>@item.iletisim</h3>
            }

        </div>
        @Html.Action("ikonlar","AnaSayfa")
    </div>

</body>
</html>